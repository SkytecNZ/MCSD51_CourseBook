<html>
  <head>
    <%- include('partials/header1.ejs') %>
  </head>
  <body>
    <%- include('partials/navbar1.ejs') %>
    <div class="container">
      <h2>Add a MP to the List</h2>
      <form method="POST">
        <label for="id"> MP ID</label>
        <input type="text" id="id" placeholder="Enter MP ID" name="id">
        <label for="name">MP Name</label>
        <input type="text" id="name" placeholder="Enter MP's Name" name="name">
        <label for="party">Political Party</label>
        <input type="text" id="party" placeholder="Political Party Name" name="party">
        <button type="submit">Add MP</button>
        <button type="reset">Reset</button>
      </form>
    </div>
  </body>
</html>